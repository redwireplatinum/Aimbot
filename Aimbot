-- Scripts:
function SCRIPT_YZND84_FAKESCRIPT() -- Aimbot.Scripts 
	local script = Instance.new('LocalScript')
	script.Parent = Aimbot
	for i,v in pairs(script.Parent:GetChildren()) do
		if v.ClassName == "Frame" then
			v.Draggable = true
		end
	end
	local EncryptedName = ""
	local wlui = script.Parent.TeamWhitelist
	wlui.WhitelistedTeams.UISizeConstraint.MaxSize = Vector2.new(324, math.huge)
	wlui.WhitelistedTeams.ScrollBarImageColor3 = Color3.new(0,1,0)
	for i = 1, 15 do
		local char = string.char(math.random(1, 255))
		EncryptedName = EncryptedName..char
	end
	script.Parent.Name = EncryptedName
	_G.AimbotGUIOpen = true
	local RedsAimbot = {}
	local lplr = game.Players.LocalPlayer
	local m = lplr:GetMouse()
	RedsAimbot.Stats = {}
	RedsAimbot.Functions = {}
	RedsAimbot.Stats.Target = "Head"
	RedsAimbot.Stats.IsOn = false
	RedsAimbot.Stats.Player = ""
	RedsAimbot.Stats.AutoTarget = false
	RedsAimbot.Stats.CenterLock = true
	RedsAimbot.Functions.deselect = function()
	RedsAimbot.Stats.Player = ""
	coroutine.resume(coroutine.create(function()
	if RedsAimbot.Stats.IsOn then
	RedsAimbot.Stats.IsOn = false
	wait()
	RedsAimbot.Stats.IsOn = true
	end
	end))
	end
	function GetPlayer(character)
	if character.Name ~= lplr.Name then
	if game.Teams:FindFirstChildOfClass("Team") and TeamIgnore(character) and game.Players[character.Name].Team ~= lplr.Team and character:FindFirstChild("Humanoid") and character.Humanoid.Health ~= 0 then
	selectplr(character)
	elseif not game.Teams:FindFirstChildOfClass("Team") and character:FindFirstChild("Humanoid") and character.Humanoid.Health ~= 0 then
	selectplr(character)
	elseif allteam() then
	selectplr(character)
	end
	end
	end
	RedsAimbot.Functions.GetNearestPlayer = function()
	local studs = 500
	local table1 = {}
	local table2 = {}
	for i,v in pairs(game.Players:GetPlayers()) do
		if v.Character and v.Name ~= lplr.Name and v.Character:FindFirstChild(RedsAimbot.Stats.Target) then
		if TeamIgnore(v) and v.Team ~= lplr.Team or allteam() then
			local pos = math.floor(lplr:DistanceFromCharacter(v.Character[RedsAimbot.Stats.Target].Position))
			local _, onscreen = workspace.Camera:WorldToScreenPoint(v.Character[RedsAimbot.Stats.Target].Position)
			if onscreen then
			table1[pos] = v.Character
			table.insert(table2, pos)
			end
		end
	end
	end
	table.sort(table2)
	for position, character in pairs(table1) do
	if table2[1] == position and position < studs then
	GetPlayer(character)
	end
	end
	end
	local Keybinds = [[
	Aimbot keybinds:
	Left alt - turns Aimbot on/off
	(Turning it on will attempt to find a player wherever your cursor hovers, if a player is found, your cursor will "forcefully" stay on the set targeted part.)
	Right alt - targets the nearest opponent within 500 studs (aimbot has to be on, obviously)
	Left click + X - Changes the targeted part (This is set to head by default)
	Auto-Targeting - Basically spams the GetNearestPlayer function
	Center-Lock mouse: Chooses whether you want your mouse to stay on the center of the screen or not (some games might not support center-locking.)
	]]
	print(Keybinds)
	Changed = function(part, value, func)
	    local current = part[value]
	    coroutine.resume(coroutine.create(function()
	        while true do
	            repeat game.RunService.RenderStepped:wait() until part[value] ~= current
	            func(part[value], current, part)
	            current = part[value]
	    end
	    end))
	end
	local TeamTable = {}
	function repack(str)
	local t = {}
	local arg = ""
	local tableseperator = " "
	str = str..tableseperator
	for i = 1, string.len(str) do
	if str:sub(i,i) ~= tableseperator then
	arg = arg..str:sub(i,i)
	else
	if tonumber(arg) then
	table.insert(t, tonumber(arg))
	else
	table.insert(t, arg)
	end
	arg = ""
	end
	end
	return t
	end
	function LockMouse()
	local plrmodule = lplr.PlayerScripts:FindFirstChild("PlayerModule")
	local cammodule
	local camsettings
	if plrmodule then
	cammodule = plrmodule:FindFirstChild("CameraModule")
	if cammodule then
	camsettings = require(cammodule)
	end
	end
	if plrmodule and cammodule and camsettings and RedsAimbot.Stats.CenterLock then
	return camsettings.activeCameraController
	else
	return nil
	end
	end
	function selectplr(chr)
	if RedsAimbot.Stats.IsOn == true and RedsAimbot.Stats.Player == "" then
	RedsAimbot.Stats.Player = chr
	if game.PlaceId == 286090429 then
	chr.ChildAdded:connect(function(c)
	if c.Name == "Cam" and RedsAimbot.Stats.Player == chr then
	chr.Humanoid.Health = 0
	chr.ChildRemoved:Wait()
	chr.Humanoid.Health = 100
	end
	end)
	end
	end
	end
	function allteam()
	local count = 0
	for i, plr in pairs(game.Players:GetPlayers()) do
	if plr.Team == lplr.Team then
	count = count + 1
	end
	end
	if count == #game.Players:GetPlayers() then
	return true
	else
	return false
	end
	end
	function TeamIgnore(character)
	local numberofargs = 0
	local AAA = 0
	for playerteam, lplrteam in pairs(TeamTable) do
	numberofargs = numberofargs + 1
	if lplr.Team == game.Teams[lplrteam] then
	if game.Players[character.Name].Team ~= game.Teams[playerteam] then
	AAA = AAA + 1
	end
	else
	AAA = AAA + 1
	end
	end
	if numberofargs == AAA then
	return true
	else
	return false
	end
	end
		m.KeyDown:connect(function()
		if game.UserInputService:IsKeyDown(Enum.KeyCode.LeftAlt) then
		if RedsAimbot.Stats.IsOn == false then
		RedsAimbot.Stats.IsOn = true
		else
		RedsAimbot.Stats.IsOn = false
		end
		end
		if game.UserInputService:IsKeyDown(Enum.KeyCode.RightAlt) then
		RedsAimbot.Functions.GetNearestPlayer()
		end
		end)
	local function message(txt)
	game.StarterGui:SetCore("SendNotification", {
	Title = "Aimbot target";
	Text = txt;
	})
	end
	if not lplr.Character then
	message("Spawn in the game for the aimbot to work")
	end
	if not lplr.Character then lplr.CharacterAdded:Wait() end
	local chr = lplr.Character
	repeat wait() until chr:FindFirstChild("Torso") or chr:FindFirstChild("UpperTorso")
	local Torso = chr:FindFirstChild("Torso") or chr:FindFirstChild("UpperTorso")
	m.Button1Down:connect(function()
	if game.UserInputService:IsKeyDown(Enum.KeyCode.X) then
	if RedsAimbot.Stats.Target == "Torso" or RedsAimbot.Stats.Target == "UpperTorso" then
	RedsAimbot.Stats.Target = "Head"
	else
	RedsAimbot.Stats.Target = Torso.Name
	end
	end
	end)
	game.RunService.RenderStepped:connect(function()
	if _G.AimbotGUIOpen then
		if RedsAimbot.Stats.Player ~= "" and RedsAimbot.Stats.Player:FindFirstChild(RedsAimbot.Stats.Target) then
		local v, _ = workspace.Camera:WorldToScreenPoint(RedsAimbot.Stats.Player[RedsAimbot.Stats.Target].Position)
	if _ then
	local StabilizeNum = 0.45
		local x, y = (v.X - m.X)*StabilizeNum, (v.Y - m.Y)*StabilizeNum
		if RedsAimbot.Stats.IsOn then
		mousemoverel(x, y)
		end
	elseif not _ then
	RedsAimbot.Functions.deselect()
	end
		end
		if m.Target ~= nil then
		if game.Players:FindFirstChild(m.Target.Parent.Name) then
		GetPlayer(m.Target.Parent)
		elseif game.Players:FindFirstChild(m.Target.Parent.Parent.Name) then
	    GetPlayer(m.Target.Parent.Parent)
		end
		end
	if RedsAimbot.Stats.AutoTarget then
	RedsAimbot.Functions.GetNearestPlayer()
	end
	if LockMouse() then
	if RedsAimbot.Stats.Player ~= "" then
	if RedsAimbot.Stats.IsOn then
	LockMouse():SetIsMouseLocked(true)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(2, 0, 0)
	else
	if game.PlaceId == 606849621 then
	    if not game.Players.LocalPlayer.PlayerGui:FindFirstChild("CrossHairGui") then
	        LockMouse():SetIsMouseLocked(false)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end
	else
	LockMouse():SetIsMouseLocked(false)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end
	end
	else
	if game.PlaceId == 606849621 then
	 if not game.Players.LocalPlayer.PlayerGui:FindFirstChild("CrossHairGui") then
	LockMouse():SetIsMouseLocked(false)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end
	else
	    LockMouse():SetIsMouseLocked(false)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end
	end
	end
	end
		end)
	game.StarterGui:SetCore("SendNotification", {
	Title = "Aimbot";
	Text = "To see how the aimbot works, check the developer log.";})
	
	function Died(player)
	if not player.Character then player.CharacterAdded:Wait() end
	local function OnDeath(chr)
	chr:WaitForChild("Humanoid")
	chr.Humanoid.Died:connect(function()
	if RedsAimbot.Stats.Player == chr then
	RedsAimbot.Functions.deselect()
	end
	end)
	end
	OnDeath(player.Character)
	player.CharacterAdded:connect(OnDeath)
	end
	for i,v in pairs(game.Players:GetPlayers()) do Died(v) end
	game.Players.PlayerAdded:connect(Died)
	game.Players.PlayerRemoving:connect(function(plr)
	if plr.Character and plr.Character == RedsAimbot.Stats.Player then
	RedsAimbot.Functions.deselect()
	end
	end)
	local MainFrame = script.Parent.MFrame
	MainFrame.X.MouseButton1Click:connect(function()
	_G.AimbotGUIOpen = false
	MainFrame.Parent:Destroy()
	end)
	MainFrame.TargetedPart.Text = "Targeted Part: "..RedsAimbot.Stats.Target
	Changed(RedsAimbot.Stats, "Target", function(v)
	MainFrame.TargetedPart.Text = "Targeted Part: "..v
	end)
	MainFrame.TargetedPart.MouseButton1Click:connect(function()
	if RedsAimbot.Stats.Target == "Torso" or RedsAimbot.Stats.Target == "UpperTorso" then
	RedsAimbot.Stats.Target = "Head"
	else
	RedsAimbot.Stats.Target = Torso.Name
	end
	end)
	Changed(RedsAimbot.Stats, "Player", function(v)
	if v ~= "" then
	MainFrame.TargetNearestPlayer.Text = "Chosen "..v.Name
	else
	MainFrame.TargetNearestPlayer.Text = "Click to open team whitelist UI"
	end
	end)
	MainFrame.TargetNearestPlayer.MouseButton1Click:connect(function()
	wlui.Visible = true
	end)
	MainFrame.LockCenterEnabled.Text = "Center-Lock Mouse: "..tostring(RedsAimbot.Stats.CenterLock)
	Changed(RedsAimbot.Stats, "CenterLock", function(v)
	MainFrame.LockCenterEnabled.Text = "Center-Lock Mouse: "..tostring(v)
	end)
	MainFrame.LockCenterEnabled.MouseButton1Click:connect(function()
	if RedsAimbot.Stats.CenterLock then
	RedsAimbot.Stats.CenterLock = false
	else
	RedsAimbot.Stats.CenterLock = true
	end
	end)
	MainFrame.IsAimbotOn.Text = "Aimbot Enabled: "..tostring(RedsAimbot.Stats.IsOn)
	Changed(RedsAimbot.Stats, "IsOn", function(v)
	MainFrame.IsAimbotOn.Text = "Aimbot Enabled: "..tostring(v)
	if v == false then
	RedsAimbot.Functions.deselect()
	end
	end)
	MainFrame.IsAimbotOn.MouseButton1Click:connect(function()
	if RedsAimbot.Stats.IsOn then
	RedsAimbot.Stats.IsOn = false
	else
	RedsAimbot.Stats.IsOn = true
	end
	end)
	MainFrame.AutoTarget.Text = "Auto-Targeting Enabled: "..tostring(RedsAimbot.Stats.AutoTarget)
	Changed(RedsAimbot.Stats, "AutoTarget", function(v)
	MainFrame.AutoTarget.Text = "Auto-Targeting Enabled: "..tostring(v)
	end)
	MainFrame.AutoTarget.MouseButton1Click:connect(function()
	if RedsAimbot.Stats.AutoTarget then
	RedsAimbot.Stats.AutoTarget = false
	else
	RedsAimbot.Stats.AutoTarget = true
	end
	end)
	wlui.about.MouseButton1Click:Connect(function()
	wlui.AboutUI.Visible = true
	end)
	wlui.AboutUI.Close.MouseButton1Click:connect(function()
	wlui.AboutUI.Visible = false
	end)
	wlui.hide.MouseButton1Click:connect(function()
	wlui.Visible = false
	end)
	wlui.setup.MouseButton1Click:Connect(function()
	local team1 = wlui.team1.Text
	local team2 = wlui.team2.Text
	local listui = wlui.wlteam:Clone()
	local Team_1 = Instance.new("StringValue", listui)
	local Team_2 = Instance.new("StringValue", listui)
	for i,team in pairs(game.Teams:GetTeams()) do
	if team.Name:lower():sub(1,team1:len()) == team1:lower() then
	Team_1.Value = team.Name
	end
	if team.Name:lower():sub(1,team2:len()) == team2:lower() then
	Team_2.Value = team.Name
	end
	end
	listui.Visible = true
	listui.Parent = wlui.WhitelistedTeams
	listui.TextLabel.Text = "The "..Team_1.Value.." team will be whitelisted when you're in the "..Team_2.Value.." team."
	TeamTable[Team_1.Value] = Team_2.Value
	listui.X.MouseButton1Click:connect(function()
	TeamTable[Team_1.Value] = nil
	listui:Destroy()
	end)
	end)

end
coroutine.resume(coroutine.create(SCRIPT_YZND84_FAKESCRIPT))
end
