TargetedPart.TextWrapped = true
-- Scripts:
function SCRIPT_CHWG70_FAKESCRIPT() -- TargetNearestPlayer.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = TargetNearestPlayer
	repeat wait() until _G.Changed and _G.RedsAimbot.Functions.GetNearestPlayer
	_G.Changed(_G.RedsAimbot.Stats, "Player", function(v)
	if v ~= "" then
	script.Parent.Text = "Chosen "..v.Name
	else
	script.Parent.Text = "Target someone within 500 studs (Or Right Alt)"
	end
	end)
	script.Parent.MouseButton1Click:connect(function()
	_G.RedsAimbot.Functions.GetNearestPlayer()
	end)

end
coroutine.resume(coroutine.create(SCRIPT_CHWG70_FAKESCRIPT))
function SCRIPT_DZVA82_FAKESCRIPT() -- IsAimbotOn.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = IsAimbotOn
	repeat wait() until _G.Changed
	script.Parent.Text = "Aimbot Enabled: "..tostring(_G.RedsAimbot.Stats.IsOn)
	_G.Changed(_G.RedsAimbot.Stats, "IsOn", function(v)
	script.Parent.Text = "Aimbot Enabled: "..tostring(v)
	if v == false then
	_G.RedsAimbot.Functions.deselect()
	end
	end)
	script.Parent.MouseButton1Click:connect(function()
	if _G.RedsAimbot.Stats.IsOn then
	_G.RedsAimbot.Stats.IsOn = false
	else
	_G.RedsAimbot.Stats.IsOn = true
	end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_DZVA82_FAKESCRIPT))
function SCRIPT_HQWZ75_FAKESCRIPT() -- X.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = X
	script.Parent.MouseButton1Click:connect(function()
	_G.AimbotGUIOpen = false
	script.Parent.Parent.Parent:Destroy()
	end)

end
coroutine.resume(coroutine.create(SCRIPT_HQWZ75_FAKESCRIPT))
function SCRIPT_JOCC86_FAKESCRIPT() -- MFrame.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = MFrame
	_G.AimbotGUIOpen = true
	_G.RedsAimbot = {}
	_G.RedsAimbot.Stats = {}
	_G.RedsAimbot.Functions = {}
	_G.RedsAimbot.Stats.Target = "Head"
	_G.RedsAimbot.Stats.IsOn = false
	_G.RedsAimbot.Stats.Player = ""
	_G.RedsAimbot.Stats.AutoTarget = false
	local Keybinds = [[
	Aimbot keybinds:
	Left alt - turns Aimbot on/off
	(Turning it on will attempt to find a player wherever your cursor hovers, if a player is found, your cursor will "forcefully" stay on the set targeted part, depending on the game, your mouse may/may not lock itself in the center of
	the screen.)
	Right alt - targets an opponent within 500 studs
	Left click + X - Changes the targeted part (This is set to head by default)
	--Chat functions
	/e teamignore [Name of team to ignore] [Team YOU'LL be on when ignoring the selected team] / Example: if [Team Name] is Criminal and [Your Team Name] is Prisoner, then anyone on the Criminal team will be ignored as a
	target when you're a Prisoner.
	/e unteamignore [Name of team] [Team you'll be on] (will stop ignoring a certain team if you already set both team to ignore and
	the team that you'll be on)
	Both chat functions support shortened names (Ex: 'Criminal' can be written as 'crim')
	]]
	print(Keybinds)
	_G.Changed = function(part, value, func)
	    local current = part[value]
	    coroutine.resume(coroutine.create(function()
	        while true do
	            repeat game.RunService.RenderStepped:wait() until part[value] ~= current
	            func(part[value], current, part)
	            current = part[value]
	    end
	    end))
	end
	local lplr = game.Players.LocalPlayer
	local m = lplr:GetMouse()
	local TeamTable = {}
	function repack(str)
	local t = {}
	local arg = ""
	local tableseperator = " "
	str = str..tableseperator
	for i = 1, string.len(str) do
	if str:sub(i,i) ~= tableseperator then
	arg = arg..str:sub(i,i)
	else
	if tonumber(arg) then
	table.insert(t, tonumber(arg))
	else
	table.insert(t, arg)
	end
	arg = ""
	end
	end
	return t
	end
	function LockMouse()
	local plrmodule = lplr.PlayerScripts:FindFirstChild("PlayerModule")
	local cammodule
	local camsettings
	if plrmodule then
	cammodule = plrmodule:FindFirstChild("CameraModule")
	if cammodule then
	camsettings = require(cammodule)
	end
	end
	if plrmodule and cammodule and camsettings then
	return camsettings.activeCameraController
	else
	return nil
	end
	end
	function selectplr(chr)
	if _G.RedsAimbot.Stats.IsOn == true and _G.RedsAimbot.Stats.Player == "" then
	_G.RedsAimbot.Stats.Player = chr
	if game.PlaceId == 286090429 then
	chr.ChildAdded:connect(function(c)
	if c.Name == "Cam" and _G.RedsAimbot.Stats.Player == chr then
	chr.Humanoid.Health = 0
	chr.ChildRemoved:Wait()
	chr.Humanoid.Health = 100
	end
	end)
	end
	end
	end
	_G.RedsAimbot.Functions.deselect = function()
	_G.RedsAimbot.Stats.Player = ""
	coroutine.resume(coroutine.create(function()
	if _G.RedsAimbot.Stats.IsOn then
	_G.RedsAimbot.Stats.IsOn = false
	wait()
	_G.RedsAimbot.Stats.IsOn = true
	end
	end))
	end
	function allteam()
	local count = 0
	for i, plr in pairs(game.Players:GetPlayers()) do
	if plr.Team == lplr.Team then
	count = count + 1
	end
	end
	if count == #game.Players:GetPlayers() then
	return true
	else
	return false
	end
	end
	function TeamIgnore(character)
	local numberofargs = 0
	local AAA = 0
	for playerteam, lplrteam in pairs(TeamTable) do
	numberofargs = numberofargs + 1
	if lplr.Team == game.Teams[lplrteam] then
	if game.Players[character.Name].Team ~= game.Teams[playerteam] then
	AAA = AAA + 1
	end
	else
	AAA = AAA + 1
	end
	end
	if numberofargs == AAA then
	return true
	else
	return false
	end
	end
	function GetPlayer(character)
	if character.Name ~= lplr.Name then
	if game.Teams:FindFirstChildOfClass("Team") and TeamIgnore(character) and game.Players[character.Name].Team ~= lplr.Team and character:FindFirstChild("Humanoid") and character.Humanoid.Health ~= 0 then
	selectplr(character)
	elseif not game.Teams:FindFirstChildOfClass("Team") and character:FindFirstChild("Humanoid") and character.Humanoid.Health ~= 0 then
	selectplr(character)
	elseif allteam() then
	selectplr(character)
	end
	end
	end
	_G.RedsAimbot.Functions.GetNearestPlayer = function()
	local studs = 500
	local table1 = {}
	local table2 = {}
	for i,v in pairs(game.Players:GetPlayers()) do
		if v.Character and v.Name ~= lplr.Name and v.Character:FindFirstChild(_G.RedsAimbot.Stats.Target) then
		if TeamIgnore(v) and v.Team ~= lplr.Team or allteam() then
			local pos = math.floor(lplr:DistanceFromCharacter(v.Character[_G.RedsAimbot.Stats.Target].Position))
			local _, onscreen = workspace.Camera:WorldToScreenPoint(v.Character[_G.RedsAimbot.Stats.Target].Position)
			if onscreen then
			table1[pos] = v.Character
			table.insert(table2, pos)
			end
		end
	end
	end
	table.sort(table2)
	for position, character in pairs(table1) do
	if table2[1] == position and position < studs then
	GetPlayer(character)
	end
	end
	end
		m.KeyDown:connect(function()
		if game.UserInputService:IsKeyDown(Enum.KeyCode.LeftAlt) then
		if _G.RedsAimbot.Stats.IsOn == false then
		_G.RedsAimbot.Stats.IsOn = true
		else
		_G.RedsAimbot.Stats.IsOn = false
		end
		end
		if game.UserInputService:IsKeyDown(Enum.KeyCode.RightAlt) then
		_G.RedsAimbot.Functions.GetNearestPlayer()
		end
		end)
	local function message(txt)
	game.StarterGui:SetCore("SendNotification", {
	Title = "Aimbot target";
	Text = txt;
	})
	end
	if not lplr.Character then
	message("Spawn in the game for the aimbot to work")
	end
	if not lplr.Character then lplr.CharacterAdded:Wait() end
	local chr = lplr.Character
	repeat wait() until chr:FindFirstChild("Torso") or chr:FindFirstChild("UpperTorso")
	local Torso = chr:FindFirstChild("Torso") or chr:FindFirstChild("UpperTorso")
	m.Button1Down:connect(function()
	if game.UserInputService:IsKeyDown(Enum.KeyCode.X) then
	if _G.RedsAimbot.Stats.Target == "Torso" or _G.RedsAimbot.Stats.Target == "UpperTorso" then
	_G.RedsAimbot.Stats.Target = "Head"
	else
	_G.RedsAimbot.Stats.Target = Torso.Name
	end
	end
	end)
	game.RunService.RenderStepped:connect(function()
	if _G.AimbotGUIOpen then
		if _G.RedsAimbot.Stats.Player ~= "" and _G.RedsAimbot.Stats.Player:FindFirstChild(_G.RedsAimbot.Stats.Target) then
		local v, _ = workspace.Camera:WorldToScreenPoint(_G.RedsAimbot.Stats.Player[_G.RedsAimbot.Stats.Target].Position)
	if _ then
	local StabilizeNum = 0.45
		local x, y = (v.X - m.X)*StabilizeNum, (v.Y - m.Y)*StabilizeNum
		if _G.RedsAimbot.Stats.IsOn then
		mousemoverel(x, y)
		end
	elseif not _ then
	_G.RedsAimbot.Functions.deselect()
	end
		end
		if m.Target ~= nil then
		if game.Players:FindFirstChild(m.Target.Parent.Name) then
		GetPlayer(m.Target.Parent)
		elseif game.Players:FindFirstChild(m.Target.Parent.Parent.Name) then
	    GetPlayer(m.Target.Parent.Parent)
		end
		end
	if _G.RedsAimbot.Stats.AutoTarget then
	_G.RedsAimbot.Functions.GetNearestPlayer()
	end
	if LockMouse() then
	if _G.RedsAimbot.Stats.Player ~= "" then
	if _G.RedsAimbot.Stats.IsOn then
	LockMouse():SetIsMouseLocked(true)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(2, 0, 0)
	else
	LockMouse():SetIsMouseLocked(false)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end
	else
	LockMouse():SetIsMouseLocked(false)
	lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
	end
	end
	end
		end)
	game.StarterGui:SetCore("SendNotification", {
	Title = "Aimbot";
	Text = "To see how the aimbot works, check the developer log.";})
	lplr.Chatted:connect(function(msg)
	if msg:sub(1,14) == "/e teamignore " then
	local args = repack(msg)
	if args[3] and args[4] then
	local teamtoignore
	local teamtoignoreteam
	for i,team in pairs(game.Teams:GetTeams()) do
	if team.Name:lower():sub(1, string.len(args[3])) == string.lower(args[3]) then
	teamtoignore = team.Name
	end
	if team.Name:lower():sub(1, string.len(args[4])) == string.lower(args[4]) then
	teamtoignoreteam = team.Name
	end
	if teamtoignore and teamtoignoreteam then
	local chat = lplr.PlayerGui.Chat.Frame.ChatChannelParentFrame.Frame_MessageLogDisplay.Scroller.ChildAdded:Wait()
	    if string.match(chat.TextLabel.Text, "Emote") then
	        chat.TextLabel.Text = "Players in the "..teamtoignore.." team will be ignored when you're in the "..teamtoignoreteam.." team."
	        chat.TextLabel.TextColor3 = Color3.new(0,1,0)
	    end
	TeamTable[teamtoignore] = teamtoignoreteam
	end
	end
	end
	end
	if msg:sub(1,16) == "/e unteamignore " then
	local args = repack(msg)
	local chat = lplr.PlayerGui.Chat.Frame.ChatChannelParentFrame.Frame_MessageLogDisplay.Scroller.ChildAdded:Wait()
	if args[3] and args[4] then
	local t1
	local t2
	for team1, team2 in pairs(TeamTable) do
	if team1:lower():sub(1, string.len(args[3])) == string.lower(args[3]) and team2:lower():sub(1, string.len(args[4])) == string.lower(args[4]) then
	t1 = team1
	t2 = team2
	TeamTable[team1] = nil
	end
	end
	    if string.match(chat.TextLabel.Text, "Emote") then
	        chat.TextLabel.Text = "Players in the "..t1.." team will no longer be ignored when you're in the "..t2.." team."
	        chat.TextLabel.TextColor3 = Color3.new(0,1,0)
	    end
	end
	end
	end)
	
	function Died(player)
	if not player.Character then player.CharacterAdded:Wait() end
	local function OnDeath(chr)
	chr:WaitForChild("Humanoid")
	chr.Humanoid.Died:connect(function()
	if _G.RedsAimbot.Stats.Player == chr then
	_G.RedsAimbot.Functions.deselect()
	end
	end)
	end
	OnDeath(player.Character)
	player.CharacterAdded:connect(OnDeath)
	end
	for i,v in pairs(game.Players:GetPlayers()) do Died(v) end
	game.Players.PlayerAdded:connect(Died)

end
coroutine.resume(coroutine.create(SCRIPT_JOCC86_FAKESCRIPT))
function SCRIPT_NHXI87_FAKESCRIPT() -- AutoTarget.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = AutoTarget
	repeat wait() until _G.Changed
	script.Parent.Text = "Auto-Targeting Enabled: "..tostring(_G.RedsAimbot.Stats.AutoTarget)
	_G.Changed(_G.RedsAimbot.Stats, "AutoTarget", function(v)
	script.Parent.Text = "Auto-Targeting Enabled: "..tostring(v)
	end)
	script.Parent.MouseButton1Click:connect(function()
	if _G.RedsAimbot.Stats.AutoTarget then
	_G.RedsAimbot.Stats.AutoTarget = false
	else
	_G.RedsAimbot.Stats.AutoTarget = true
	end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_NHXI87_FAKESCRIPT))
function SCRIPT_RTLB85_FAKESCRIPT() -- TargetedPart.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = TargetedPart
	script.Parent.Text = "Loading..."
	repeat wait() until _G.Changed
	local lplr = game.Players.LocalPlayer
	if not lplr.Character then lplr.CharacterAdded:Wait() end
	local chr = lplr.Character
	repeat wait() until chr:FindFirstChild("Torso") or chr:FindFirstChild("UpperTorso")
	script.Parent.Text = "Targeted Part: ".._G.RedsAimbot.Stats.Target
	local Torso = chr:FindFirstChild("Torso") or chr:FindFirstChild("UpperTorso")
	_G.Changed(_G.RedsAimbot.Stats, "Target", function(v)
	script.Parent.Text = "Targeted Part: "..v
	end)
	script.Parent.MouseButton1Click:connect(function()
	if _G.RedsAimbot.Stats.Target == "Torso" or _G.RedsAimbot.Stats.Target == "UpperTorso" then
	_G.RedsAimbot.Stats.Target = "Head"
	else
	_G.RedsAimbot.Stats.Target = Torso.Name
	end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_RTLB85_FAKESCRIPT))
function SCRIPT_YJRI75_FAKESCRIPT() -- Aimbot.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = Aimbot
	for i,v in pairs(script.Parent:GetChildren()) do
		if v.ClassName == "Frame" then
			v.Draggable = true
		end
	end
	local EncryptedName = ""
	for i = 1, 15 do
		local char = string.char(math.random(1, 255))
		EncryptedName = EncryptedName..char
	end
	script.Parent.Name = EncryptedName

end
coroutine.resume(coroutine.create(SCRIPT_YJRI75_FAKESCRIPT))
end
