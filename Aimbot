if not game:FindFirstChild("RedWiresAimBot") then
local v = Instance.new("Part", game)
v.Name = "RedWiresAimBot"
local Keybinds = [[
Aimbot keybinds:
Left alt - turns Aimbot on/off
(Turning it on will attempt to find a player wherever your cursor hovers, if a player is found, your cursor will "forcefully" stay on the set targeted part, depending on the game, your mouse may/may not lock itself in the center of
the screen.)
Right alt - targets an opponent within 500 studs
Left click + X - Changes the targeted part (This is set to head by default)
]]
local Aimbot = Instance.new("ScreenGui")
local MFrame = Instance.new("Frame")
local Select = Instance.new("TextButton")
local ison = Instance.new("TextButton")
local X = Instance.new("TextButton")
local on = Instance.new("BoolValue", MFrame)
local plr = Instance.new("StringValue", MFrame)
local lplr = game.Players.LocalPlayer
local target = Instance.new("StringValue", MFrame)
target.Name = "target"
target.Value = "Head"
local m = lplr:GetMouse()
function GetPlayer(player)
if game.Players:FindFirstChild(player.Name) and player.Name ~= lplr.Name then
if game.Teams:FindFirstChildOfClass("Team") and game.Players[player.Name].Team ~= lplr.Team and game.Players[player.Name].Character.Humanoid.Health ~= 0 then
selectplr(player.Name)
elseif not game.Teams:FindFirstChildOfClass("Team") and game.Players[player.Name].Character.Humanoid.Health ~= 0 then
selectplr(player.Name)
elseif allteam() then
selectplr(player.Name)
end
end
end
function allteam()
local count = 0
for i, plr in pairs(game.Players:GetPlayers()) do
if plr.Team == lplr.Team then
count = count + 1
end
end
if count == #game.Players:GetPlayers() then
return true
else
return false
end
end
function fuck()
local plrmodule = lplr.PlayerScripts:FindFirstChild("PlayerModule")
local cammodule
local camsettings
if plrmodule then
cammodule = plrmodule:FindFirstChild("CameraModule")
if cammodule then
camsettings = require(cammodule)
end
end
if plrmodule and cammodule and camsettings then
return camsettings.activeCameraController
else
return nil
end
end
function deselect()
if fuck() then
fuck():SetIsMouseLocked(false)
lplr.Character.Humanoid.CameraOffset = Vector3.new(0, 0, 0)
end
plr.Value = ""
Select.Text = "Click to target someone within 500 studs. (turn aimbot on)"
end
function selectplr(name)
if fuck() then
fuck():SetIsMouseLocked(true)
lplr.Character.Humanoid.CameraOffset = Vector3.new(2, 0, 0)
end
	plr.Value = name
	Select.Text = "Chosen "..plr.Value
end
game.RunService.Heartbeat:connect(function()
if game.CoreGui:FindFirstChild("Aimbot") then
	if plr.Value ~= "" and game.Players:FindFirstChild(plr.Value) then
	local v, _ = workspace.Camera:WorldToScreenPoint(game.Players[plr.Value].Character[target.Value].Position)
if _ then
	local x = v.X * 0.45 - m.X * 0.45
	local y = v.Y * 0.45 - m.Y * 0.45
	if on.Value == true then
	mousemoverel(x, y)
	end
elseif not _ then
deselect()
end
	end
	if m.Target ~= nil then
	if plr.Value == "" and on.Value == true then
	if game.Players:FindFirstChild(m.Target.Parent.Name) then
	GetPlayer(m.Target.Parent)
	elseif game.Players:FindFirstChild(m.Target.Parent.Parent.Name) then
    GetPlayer(m.Target.Parent.Parent)
	end
	end
	end
end
	end)
--Properties:
plr:GetPropertyChangedSignal("Value"):connect(function()
if game.Players:FindFirstChild(plr.Value) then
local chr = game.Players[plr.Value].Character
if game.PlaceId == 286090429 then
chr.ChildAdded:connect(function(c)
if c.Name == "Cam" then
chr.Humanoid.Health = 0
end
end)
end
chr.Humanoid.Died:connect(function()
deselect()
end)
end
end)
m.Button1Down:connect(function()
function message(txt)
game.StarterGui:SetCore("SendNotification", {
Title = "Aimbot target";
Text = txt;
})
end
if game.UserInputService:IsKeyDown(Enum.KeyCode.X) then
if target.Value == "Head" then
target.Value = "HumanoidRootPart"
message("Target part changed to HumanoidRootPart")
else
target.Value = "Head"
message("Target part changed to Head")
end
end
end)
Aimbot.Name = "Aimbot"
Aimbot.Parent = game.CoreGui
Aimbot.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

on.Name = "on"
on.Value = false

plr.Name = "plr"
plr.Value = ""

MFrame.Name = "MFrame"
MFrame.Parent = Aimbot
MFrame.Active = true
MFrame.BackgroundColor3 = Color3.new(0.462745, 0.462745, 0.462745)
MFrame.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
MFrame.Position = UDim2.new(0.446167886, 0, 0.0224883445, 0)
MFrame.Size = UDim2.new(0, 232, 0, 81)

Select.Name = "Select"
Select.Parent = MFrame
Select.BackgroundColor3 = Color3.new(0.462745, 0.462745, 0.462745)
Select.Position = UDim2.new(0, 0, 0.377166152, 0)
Select.Size = UDim2.new(0, 183, 0, 50)
Select.Font = Enum.Font.Code
Select.Text = "Click to target someone within 500 studs. (turn aimbot on)"
Select.TextColor3 = Color3.new(0, 0, 0)
Select.TextScaled = true
Select.TextSize = 14
Select.TextWrapped = true

ison.Name = "ison"
ison.Parent = MFrame
ison.BackgroundColor3 = Color3.new(0.462745, 0.462745, 0.462745)
ison.Position = UDim2.new(0.788793087, 0, 0.377166152, 0)
ison.Size = UDim2.new(0, 49, 0, 50)
ison.Font = Enum.Font.Code
ison.Text = "Aimbot on: "
ison.TextColor3 = Color3.new(0, 0, 0)
ison.TextScaled = true
ison.TextSize = 14
ison.TextWrapped = true

X.Name = "X"
X.Parent = MFrame
X.BackgroundColor3 = Color3.new(1, 1, 1)
X.BackgroundTransparency = 1
X.Size = UDim2.new(0, 29, 0, 29)
X.Font = Enum.Font.SourceSans
X.Text = "X"
X.TextColor3 = Color3.new(1, 0, 0)
X.TextScaled = true
X.TextSize = 14
X.TextWrapped = true
-- Scripts:
function SCRIPT_KKJF67_FAKESCRIPT() -- X.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = X
	script.Parent.MouseButton1Down:connect(function()
	script.Parent.Parent.Parent:Destroy()
	end)

end
function getnearbyplr()
if plr.Value == "" and on.Value == true then
local studs = 500
local nearplr
for i,v in pairs(game.Players:GetPlayers()) do
	if v.Character and v.Name ~= game.Players.LocalPlayer.Name then
	if lplr.Team ~= v.Team or allteam() then
		local pos = lplr:DistanceFromCharacter(v.Character.HumanoidRootPart.Position)
		local _, onscreen = workspace.Camera:WorldToScreenPoint(v.Character[target.Value].Position)
		if pos < studs and onscreen then
			nearplr = v
		end
	end
	end
end
if nearplr then
selectplr(nearplr.Name)
end
end
end
coroutine.resume(coroutine.create(SCRIPT_KKJF67_FAKESCRIPT))
function SCRIPT_DYHS71_FAKESCRIPT() -- MFrame.LocalScript 
	local script = Instance.new('LocalScript')
	script.Parent = MFrame
	script.Parent.Draggable = true
	local on = script.Parent.on
	local txt = script.Parent.ison
	local msg = "Aimbot on: "
	txt.Text = msg.."false"
	m.KeyDown:connect(function()
	if game.UserInputService:IsKeyDown(Enum.KeyCode.LeftAlt) then
	if on.Value == false then
	on.Value = true
	txt.Text = msg.."true"
	else
	on.Value = false
	txt.Text = msg.."false"
	end
	end
	if game.UserInputService:IsKeyDown(Enum.KeyCode.RightAlt) then
	getnearbyplr()
	end
	end)

end
coroutine.resume(coroutine.create(SCRIPT_DYHS71_FAKESCRIPT))
Select.MouseButton1Down:connect(function()
getnearbyplr()
end)
on:GetPropertyChangedSignal("Value"):connect(function()
   	if on.Value == false then
	deselect()
	end
end)
print(Keybinds)
game.StarterGui:SetCore("SendNotification", {
Title = "Keybinds";
Text = "To see how the aimbot works, check the developer log.";})
else
game.StarterGui:SetCore("SendNotification", {
Title = "Aimbot";
Text = "Executing Aimbot twice will mess up the aiming function.";})
end
