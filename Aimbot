-- Farewell Infortality.
-- Version: 2.82
-- Instances:
if not _G.RedsAimbot then
local Aimbot = Instance.new("ScreenGui")
local MFrame = Instance.new("Frame")
local TargetNearestPlayer = Instance.new("TextButton")
local IsAimbotOn = Instance.new("TextButton")
local X = Instance.new("TextButton")
local AutoTarget = Instance.new("TextButton")
local Title = Instance.new("TextLabel")
local TargetedPart = Instance.new("TextButton")
--Properties:
Aimbot.Name = "Aimbot"
Aimbot.Parent = game.CoreGui
Aimbot.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

MFrame.Name = "MFrame"
MFrame.Parent = Aimbot
MFrame.Active = true
MFrame.BackgroundColor3 = Color3.new(0, 0, 0)
MFrame.BorderColor3 = Color3.new(0, 0.666667, 0)
MFrame.Position = UDim2.new(0.66648984, 0, 0.112894237, 0)
MFrame.Size = UDim2.new(0, 260, 0, 97)

TargetNearestPlayer.Name = "TargetNearestPlayer"
TargetNearestPlayer.Parent = MFrame
TargetNearestPlayer.BackgroundColor3 = Color3.new(0, 0, 0)
TargetNearestPlayer.BorderColor3 = Color3.new(0, 0.666667, 0)
TargetNearestPlayer.Position = UDim2.new(0, 0, 0.480258912, 0)
TargetNearestPlayer.Size = UDim2.new(0, 88, 0, 50)
TargetNearestPlayer.Font = Enum.Font.Highway
TargetNearestPlayer.Text = "Target someone within 500 studs (Or Right Alt)"
TargetNearestPlayer.TextColor3 = Color3.new(0, 0.666667, 0)
TargetNearestPlayer.TextScaled = true
TargetNearestPlayer.TextSize = 14
TargetNearestPlayer.TextWrapped = true

IsAimbotOn.Name = "IsAimbotOn"
IsAimbotOn.Parent = MFrame
IsAimbotOn.BackgroundColor3 = Color3.new(0, 0, 0)
IsAimbotOn.BorderColor3 = Color3.new(0, 0.666667, 0)
IsAimbotOn.Position = UDim2.new(0.703846157, 0, 0.480258912, 0)
IsAimbotOn.Size = UDim2.new(0, 76, 0, 50)
IsAimbotOn.Font = Enum.Font.Highway
IsAimbotOn.Text = "Aimbot Enabled: "
IsAimbotOn.TextColor3 = Color3.new(0, 0.666667, 0)
IsAimbotOn.TextScaled = true
IsAimbotOn.TextSize = 14
IsAimbotOn.TextWrapped = true

X.Name = "X"
X.Parent = MFrame
X.BackgroundColor3 = Color3.new(0, 0, 0)
X.BorderColor3 = Color3.new(0, 0.666667, 0)
X.Position = UDim2.new(0.823076904, 0, 0, 0)
X.Size = UDim2.new(0, 46, 0, 46)
X.Font = Enum.Font.Highway
X.Text = "X"
X.TextColor3 = Color3.new(1, 0, 0)
X.TextScaled = true
X.TextSize = 14
X.TextWrapped = true

AutoTarget.Name = "AutoTarget"
AutoTarget.Parent = MFrame
AutoTarget.BackgroundColor3 = Color3.new(0, 0, 0)
AutoTarget.BorderColor3 = Color3.new(0, 0.666667, 0)
AutoTarget.Position = UDim2.new(0.338461548, 0, 0.480258912, 0)
AutoTarget.Size = UDim2.new(0, 95, 0, 50)
AutoTarget.Font = Enum.Font.Highway
